{
  "Gestor_v1.json": 

							{
							  "title": "FirmaEjemplo_v1",
							  "templateVersion": "test",
							  "argumentList": [
							  	"documentos",
							    "numeroTramite",
							    "nombreUsuario",
							    "urlBase",
							    "urlDocumento",
							    "urlDestinoDocumento",
							    "cl"
							  ],
							  "data": {
							    "signatureType": "PDF",
							    "pdfSignatureConfig": {
							      "VisibilityConfig": {
							        "signaturePage": "signaturesPage"
							      },
							      "reason": "Firmado por ${nombreUsuario}, para el tramite $numeroTramite, {$DATE}"
							    },
							    "certConfig": {
							      "certificateLocation": "${cl}",
							      "customPath": "${urlBase}/${cert}"
							    },
							    "formFields": {
							      "values": {
							        "cuil_habilitador": "20354414040",
							        "nombre_habilitador": "Ramos Ignacio",
							        "cuil_or": "20362650969",
							        "nombre_or": "Croppi Guillermo"
							      }
							    },
							    "fileOrigins": {
							      "remote": {
							        "URI": "${urlDocumento}",
							        "method": "GET",
							        "fileList": "${documentos}",
							        "headers": {
							          "Accept": "application/pdf"
							        }
							      }
							    },
							    "fileDestination": {
							      "remote": {
							        "URI": "${urlDestinoDocumento}",
							        "method": "POST",
							        "fileField": "archivo",
							        "formFields": {
							          "tramite": "$numeroTramite",
							          "firmado": true,
							          "idArchivo": "{$FILE_ID}"
							        },
							        "headers": {
							          "Authorization": "Basic cm9vdEBwanNmOjFBcXFxcXFx"
							        }
							      }
							    },
							    "interfaz": {
							      "title": "Firmado de documentos simple",
							      "username": "$nombreUsuario"
							    },
								"sharedHeaders": {
									"Authorization": "Basic cm9vdEBwanNmOjFBcXFxcXFx"
								},
							    "template": {
							      "id": "webtestplantilla_01",
							      "expiration": "1234567890",
							      "createdAt": "1247567890"
							    }
							  },
							  "signature": ""
							}
}